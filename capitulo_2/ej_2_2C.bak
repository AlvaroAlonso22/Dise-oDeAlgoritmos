#lang racket
(define (bob-box? x)
  (eq? (type-tag x) 'bob-box))

(define (alice-box? x)
  (eq? (type-tag x) 'alice-box))

;; Procedimientos gen√©ricos width, height y area 
(define (width box)
  (cond
    ((bob-box? box) (bob-width box))
    ((alice-box? box) (alice-width box))))

(define (height box)
  (cond
    ((bob-box? box) (bob-height box))
    ((alice-box? box) (alice-height box))))

(define (area box)
  (cond
    ((bob-box? box) (bob-area box))
    ((alice-box? box) (alice-area box))))

